#Excercise 7 Some Counting (coding is cool)

print("0 to 50")
for i in range(0, 51):
    print(i)

print("\n50 to 0")
for i in range(50, -1, -1):
    print(i)

print("\n30 to 50")
for i in range(30, 51):
    print(i)

print("\n50 down to 10 by 2")
for i in range(50, 9, -2):
    print(i)

print("\n100 to 200 by 5")
for i in range(100, 201, 5):
    print(i)
